/* components/Login/Login.wxss */
.page-container{
  display: flex;
  flex-direction: column;
  align-items: center;
}

.page-logo{
  width: 400rpx;
  height: 96rpx;
  margin: 92rpx 0 96rpx;
}

.page-section{
  width: 628rpx;
  background: #FFFFFF;
  border-radius: 12rpx;
  padding: 30rpx;
  margin-bottom: 20rpx;

}



.button-wrap{
  display: flex;
  justify-content: center;
  margin: 44rpx 0 56rpx;
}

.theme-button{
  width: 490rpx;
  height: 80rpx;
  margin-top: 26rpx;
  border-radius: 80rpx;
  box-shadow: 0 5rpx 10rpx 0 rgba(250,145,32,0.42);
}
